{
    "manifest_version": 2,
    "name": "GetInstagramPhoto",
    "version": "0.2",
    "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
      "default_popup":"popup.html"
  },
  "permissions": [
      "tabs"
    ]
  }